 @article{arakawa:18,
-  title={DQN-Tamer: Human-in-the-Loop Reinforcement Learning with Intractable Feedback},
+  title={Dqn-tamer: Human-in-the-loop reinforcement learning with intractable feedback},
   author={Arakawa, Riku and Kobayashi, Sosuke and Unno, Yuya and Tsuboi, Yuta and Maeda, Shin-ichi},
